{
  "name": "Digital Dopamine",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": ["<all_urls>", "http://localhost:3000/"]
}